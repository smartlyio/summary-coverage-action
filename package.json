    "axios": "0.27.2",
    "glob": "8.0.3"
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.1.0",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
